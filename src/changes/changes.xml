<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://maven.apache.org/changes/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
  <body>
    <!--
    WICHTIG! - Daniel Bor - 29.11.2011
    Wenn das neue JASMIN-Modul 2.5.0 verwendet wird,
    müssen noch Änderungen in der depend.xml und der metatags.xml vorgenommen werden
    -->
    <release date="2012-08-xx" version="3.0.0">
      <action date="2012-08-24" dev="mhm" type="update">
        Changed "svn" to "scm" in order to support git. "scm" starts with "scm:svn" for svn repositories,
        and with "scm:git" for git repositories.
      </action>
      <action date="2012-08-24" dev="mhm" type="update">
        Changed groupId and package to net.sf.beezle.
      </action>
      <action date="2012-08-22" dev="mhm" type="update">
        Update sushi 2.7 to 3.0.
      </action>
      <action date="2012-08-22" dev="mhm" type="add">
        Added Sass processing for all css files.
      </action>
      <action date="2012-03-30" dev="mhm" type="add">
        Added "call" comments to replace base LOCALHOST.
      </action>
      <action date="2012-03-30" dev="mhm" type="remove">
        Base LOCALHOST is not dumped but issues a deprecated warning.
      </action>
      <action date="2012-03-30" dev="mhm" type="remove">
        Pustefix Repository Projects are no longer suppored.
        Base "PROJECT" and the parameters "project" and "projectDescriptor" have been removed.
      </action>
      <action date="2012-03-30" dev="mhm" type="update">
        Update yuicompressor 2.3.6 to 2.4.6.
      </action>
      <action date="2012-03-29" dev="mhm" type="update">
        Report error for libraries without wsd.properties.
      </action>
    </release>
    <release date="2012-07-05" version="2.5.10">
      <action date="2012-07-05" dev="mhm" type="remove">
        Remove getTooLong exception.
      </action>
    </release>
    <release date="2012-06-21" version="2.5.9">
      <action date="2012-06-21" dev="nsteiner" type="fix">
        Fixed runtime misbehaviour. (next try)
      </action>
    </release>
    <release date="2012-06-20" version="2.5.8">
      <action date="2012-06-20" dev="nsteiner" type="fix">
        Fixed runtime misbehaviour. (wrong release candidate)
      </action>
    </release>
    <release date="2012-06-11" version="2.5.7">
      <action date="2012-06-11" dev="mhm" type="fix">
        Security - do not show absolute paths in source comments.
      </action>
    </release>
    <release date="2012-03-xx" version="2.5.6">
      <action date="2012-03-07" dev="mhm" type="update">
        Update Sushi 2.5.1 to 2.6.2 to fix Zip.node.length NPE.
      </action>
    </release>
    <release date="2012-03-01" version="2.5.5">
      <action date="2012-03-01" dev="mhm" type="update">
        Update Parent 1.10.1 to 1.10.4.
      </action>
      <action date="2012-02-29" dev="mhm" type="update">
        Update Life Module 0.16.5 to 0.18.10.
      </action>
    </release>
    <release date="2012-02-28" version="2.5.4">
      <action date="2012-02-28" dev="nsteiner" type="add">
        added support for pfx:include in jasmin_conf config part
      </action>
    </release>
    <release date="2012-01-09" version="2.5.3">
      <action date="2012-01-09" dev="mhm" type="update">
        Remove retry warning.
      </action>
      <action date="2011-12-30" dev="nsteiner" type="update">
        updated labjs to new version -B
      </action>
    </release>
    <release date="2011-12-19" version="2.5.2">
      <action date="2011-12-19" dev="mhm" type="fix">
        Set basedir for FileCheck.
      </action>
    </release>
    <release date="2011-12-14" version="2.5.1">
      <action date="2011-12-14" dev="mhm" type="fix">
        Fix FileChecker Docroot and Log4j config.
      </action>
    </release>
    <release date="2011-11-29" version="2.5.0">
      <action date="2011-11-29" dev="dbor" type="update">
        refactoring frontend-popups-de ressources and new jasmin version
      </action>
    </release>
    <release date="2011-09-26" version="2.4.21">
      <action date="2011-09-26" dev="nsteiner" type="fix">
        Fixed misformed jasmin request bug.
      </action>
    </release>
    <release date="2011-07-25" version="2.4.20">
      <action date="2011-07-25" dev="mhm" type="update">
        Update parent pom 1.7.2 to 1.7.3.
      </action>
      <action date="2011-07-25" dev="mhm" type="fix">
        Fix deprecated module descriptor.
      </action>
    </release>
    <release date="2011-07-25" version="2.4.19">
      <action date="2011-07-25" dev="nsteiner" type="update">
        Updated pom version for new release attempt.
      </action>
    </release>
    <release date="2011-07-22" version="2.4.18">
      <action date="2011-07-22" dev="nsteiner" type="add">
        Added navitree switch.
      </action>
    </release>
    <release date="2011-07-01" version="2.4.17">
      <action date="2011-07-01" dev="mhm" type="update">
        Update logging configuration: dump LOGGER_JASMIN, use class-name logger instead
      </action>
    </release>
    <release date="2011-06-27" version="2.4.16">
      <action dev="mhm" type="update">
        Update Sushi 2.4.0 to 2.5.1.
      </action>
      <action dev="mhm" type="update">
        Issue warning when 'get' takes more than 5 seconds.
      </action>
      <action dev="mhm" type="update">
        Dump warning sender, send (mostly) normal logging instead.
      </action>
      <action dev="mhm" type="update">
        Issue warning for missing wsd.properties.
      </action>
      <action dev="mhm" type="fix">
        Fix auto-reload for files with modified dates in the future.
      </action>
      <action dev="mhm" type="fix">
        Send proper Last-Modified header and fix last modified date computation.
      </action>
    </release>
    <release date="2011-05-12" version="2.4.15">
      <action dev="mhm" type="fix">
        Remove Byte Order Stripping, fix incoding instead.
      </action>
    </release>
    <release date="2011-05-12" version="2.4.14">
      <action dev="mhm" type="fix">
        Try to fix Byte Order Stripping.
      </action>
    </release>
    <release date="2011-05-12" version="2.4.13">
      <action dev="mhm" type="add">
        Strip Byte Order Marker before serving files (normal and minimized).
      </action>
    </release>
    <release date="2011-05-12" version="2.4.12">
      <action dev="nsteiner" type="fix">
        Removed buggy minifeid version.
      </action>
    </release>
    <release date="2011-05-12" version="2.4.11">
      <action dev="nsteiner" type="fix">
        Fixed double code merge bug in labjs min version.
      </action>
      <action dev="nsteiner" type="update">
        Update labjs version to 1.2.0.
      </action>
    </release>
    <release date="2011-05-11" version="2.4.10">
      <action dev="mhm" type="update">
        No longer catch runtime exception around date parsing.
      </action>
      <action dev="mhm" type="update">
        Update Sushi 2.3.0 to 2.4.0 to get Windows and multi-threading fixes.
      </action>
    </release>
    <release date="2011-05-02" version="2.4.9">
      <action dev="mhm" type="fix">
        Jasmin Wire Log fix.
      </action>
    </release>
    <release date="2011-05-02" version="2.4.8">
      <action dev="mhm" type="add">
        Jasmin Wire Log.
      </action>
    </release>
    <release date="2011-04-29" version="2.4.7">
      <action dev="mhm" type="fix">
        Work-around for NoHttpResponseException.
      </action>
    </release>
    <release date="2011-04-29" version="2.4.6">
      <action dev="mhm" type="fix">
        Minimization was not thread-safe.
      </action>
    </release>
    <release date="2011-04-29" version="2.4.5">
      <action dev="mhm" type="fix">
        Date parsing was not thread-safe.
      </action>
    </release>
    <release date="2011-04-29" version="2.4.4">
      <action dev="mhm" type="fix">
        Compare "gone" date with minimum of both startup dates.
      </action>
      <action dev="mhm" type="fix">
        Request date parsing more robust; and also send a warning.
      </action>
    </release>
    <release date="2011-04-28" version="2.4.3">
      <action dev="mhm" type="add">
        Configuration: test context init parameter "mode" if autoReload parameter is not specified,
        and enable autoReload if mode is not "prod". There's usually no need to specify autoReload now.
      </action>
      <action dev="mhm" type="update">
        Properly fix getLastModified().
      </action>
    </release>
    <release date="2011-04-19" version="2.4.2">
      <action dev="mhm" type="fix">
        Work-around Webdav.getLastModified warning.
      </action>
    </release>
    <release date="2011-04-14" version="2.4.1">
      <action dev="steiner" type="update">
        works now!!!
      </action>
    </release>
    <release date="2011-04-12" version="2.4.0">
      <action dev="mhm" type="update">
        Rename package from de.ui.jasmin to com.oneandone.jasmin. This affects the servlet definition in
        WEB-INF/web.xml, the namespace declaration in WEB-INF/depend.xml,
      </action>
      <action dev="mhm" type="fix">
        Servlet.getLastModified was broken (it wasn't adjusted to new paths).
      </action>
      <action dev="mhm" type="add">
        Add pre-minimized resources.
      </action>
      <action dev="mhm" type="add">
        Implicit module definitions.
      </action>
      <action dev="mhm" type="update">
        Resolve dependencies on startup, not on request.
      </action>
      <action dev="mhm" type="update">
        Resolve files on startup, not on request.
      </action>
      <action dev="mhm" type="fix">
        Fix Last-Modified header in servlet response.
      </action>
      <action dev="mhm" type="add">
        Json interface (replaces old xml repository report).
      </action>
      <action dev="mhm" type="update">
        Send http response "gone", if a request is outdated. Outdated is 7 or more days older than the other vm version.
      </action>
      <action dev="mhm" type="update">
        Move admin pages from /xml/jasmin/ to /xml/jasmin/admin/.
      </action>
      <action dev="mhm" type="update">
        Update Sushi 1.3.20 to 2.3.0.
      </action>
    </release>
    <release date="2011-01-31" version="2.3.5">
      <action dev="mhm" type="update">
        Limit error mails to the last 7 days.
      </action>
    </release>
    <release date="2011-01-26" description="raised unifiedlogging" version="2.3.4">
      <action date="2011-01-26" dev="haraldgross" type="update">
        Raised unifiedlogging to version 1.0.6.
      </action>
      <action date="2011-01-26" dev="haraldgross" type="update">
        Use latest parent pom.
      </action>
    </release>
    <release date="2011-01-11" version="2.3.3">
      <action dev="steiner" type="add">
        Added labjs support.
      </action>
    </release>
    <release date="2010-10-07" version="2.3.2">
      <action dev="mhm" type="update">
        Use latest parent pom.
      </action>
    </release>
    <release date="2010-09-09" version="2.3.1">
      <action dev="stes" type="fix">
        Live Jar fix.
      </action>
    </release>
    <release date="2010-08-03" version="2.3.0">
      <action dev="mhm" type="update">
        Use latest parent pom.
      </action>
      <action dev="mhm" type="update">
        Dump Pustefix dependency and use Pustefix-less Unified Logging.
      </action>
      <action dev="mhm" type="update">
        Move to groupId com.oneandone.
      </action>
      <action dev="mhm" type="remove">
        Dump support for parameter projectModules.
      </action>
    </release>
    <release date="2010-04-26" version="2.2.10">
      <action dev="mhm" type="update">
        Use latest parent pom.
      </action>
    </release>
    <release date="2010-04-22" version="2.2.9">
      <action dev="mhm" type="update">
        Update dependencies.
      </action>
    </release>
  </body>
</document>
